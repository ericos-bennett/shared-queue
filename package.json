{
  "name": "shared-queue",
  "version": "1.0.0",
  "description": "A simple queue for shared Spotify listening sessions",
  "main": "index.js",
  "scripts": {
    "server": "nodemon src/server/index.ts",
    "dist": "node dist/server/index.js",
    "build": "tsc -p .",
    "client": "cd src/client && npm run start",
    "build:client": "cd src/client && npm run build",
    "iall": "npm i && cd src/client && npm i",
    "startboth": "npm run server & npm run client",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericos-bennett/shared-queue.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ericos-bennett/shared-queue/issues"
  },
  "homepage": "https://github.com/ericos-bennett/shared-queue#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.37",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
