  "dependencies": {
    "@types/spotify-api": "^0.0.8",
    "axios": "^0.21.1",
    "base64url": "^3.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "query-string": "^7.0.0",
    "socket.io": "^4.0.1",
    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
