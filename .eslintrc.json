{
  "extends": ["plugin:react/recommended", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "plugins": ["prettier", "react"],
  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "prettier/prettier": "error"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
